/*View 2 and 3 Styles*/

QPushButton#m_ptr_view_2_previous_day_push_button, QPushButton#m_ptr_view_3_previous_month_push_button{background-image: url(:/images/previous_btn.png); background-repeat:no-repeat; width:41px; height:36px; border:0; }

QPushButton::pressed#m_ptr_view_2_previous_day_push_button, QPushButton::pressed#m_ptr_view_3_previous_month_push_button{background-image: url(:/images/previous_btn_pressed.png); background-repeat:no-repeat; width:41px; height:36px; border:0;}


QPushButton#m_ptr_view_2_next_day_push_button, QPushButton#m_ptr_view_3_next_month_push_button{background-image: url(:/images/next_btn.png); background-repeat:no-repeat; width:41px; height:36px; border:0;}

QPushButton::pressed#m_ptr_view_2_next_day_push_button, QPushButton::pressed#m_ptr_view_3_next_month_push_button{background-image: url(:/images/next_btn_pressed.png); background-repeat:no-repeat; width:41px; height:36px; border:0;}


QPushButton#m_ptr_view_2_next_day_push_button__disabled, QPushButton#m_ptr_view_3_next_month_push_button__disabled, QPushButton#m_ptr_view_4_next_year_push_button__disabled{background-image: url(:/images/next_btn_disabled.png); background-repeat:no-repeat; width:41px; height:36px; border:0;}



QPushButton#m_ptr_view_2_note_push_button{background-image: url(:/images/note_icon.png); background-repeat:no-repeat; width:41px; height:36px; border:0;}

QPushButton#m_ptr_view_2_note_push_off_button{background-image: url(:/images/note_icon_off.png); background-repeat:no-repeat; width:41px; height:36px; border:0;}


QLabel#m_ptr_view_2_date_label, QLabel#m_ptr_view_3_month_label{color: rgb(255, 255, 255); font: 16pt Whitney Black;}



QLabel#m_ptr_view_2_high_temperature_label{color: rgb(255, 141, 197); font:11pt Whitney Semibold;}

QLabel#m_ptr_view_2_low_temperature_label{color: rgb(255, 141, 197); font:11pt Whitney Semibold;}


QLabel#lblTotal{color: rgb(255, 141, 197); font:10pt Whitney; padding:5px 0 0 0;}

QLabel#lblTotalValue{color: rgb(255, 255, 255); font:15pt Whitney Black; margin:0 5px 0 0;}


