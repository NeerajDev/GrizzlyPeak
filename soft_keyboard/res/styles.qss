QLabel#lblDailyData{color: rgb(255, 255, 255); font: bold 11pt Whitney;}

QLabel#lbltoolTip{background-image: url(:/images/tooltip_bg.png); background-repeat:no-repeat; width:178px; height:93px; border:0;}

QLabel#lblenergySourceTitle{color: rgb(136, 36, 89); font: 15pt Whitney Black;}

QLabel#lblenergySourceConsumption{color: rgb(201, 72, 135); font: 11pt Whitney;}


