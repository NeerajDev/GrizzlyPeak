QLabel#lblSeperator, QLabel#lblSeperator2{ border-top: 1px solid rgb(149,43,98); border-bottom: 1px solid rgb(162,50,107);}



/*Keyboard Styles*/


QWidget#WidgetKeyboard{background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(120, 32, 79, 255), stop:0.93 rgba(151, 40, 99, 255), stop:1 rgba(187, 50, 122, 255));}

QToolButton{background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(59, 59, 59, 55), stop:0.08 rgba(0, 0, 0, 55), stop:1 rgba(166, 166, 166, 55)); border-radius: 5px; font: 20pt Whitney Book;  color: rgb(255, 255, 255);  padding:18px 8px 18px 8px; border: 1px solid rgb(101,27,66);  border-top: 1px solid rgb(221,124,176);  border-bottom: 2px solid rgb(64,18,43);}


QToolButton::pressed{background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(187, 50, 122, 255), stop:0.142391 rgba(151, 40, 99, 255), stop:1 rgba(120, 32, 79, 255));}


QPushButton#m_ptr_minimize_keyboard_button{background-image: url(:/images/minimize_keyboard_icon.png); background-repeat:no-repeat; border:0; background-position:center; background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(186, 49, 122, 255), stop:0.890212 rgba(192, 70, 135, 255));  border-top-left-radius: 10px; border-top-right-radius: 10px; border-top: 1px solid rgb(221,124,176);  border-bottom: 0px;  border-right: 0px; border-left: 2px solid rgb(118,31,77);}

QPushButton#m_ptr_maximize_keyboard_button{background-image: url(:/images/maximize_keyboard_icon.png); background-repeat:no-repeat; border:0; background-position:center; background-color: qlineargradient(spread:pad, x1:0.5, y1:1, x2:0.5, y2:0, stop:0 rgba(186, 49, 122, 255), stop:0.890212 rgba(192, 70, 135, 255));  border-top-left-radius: 10px; border-top-right-radius: 10px; border-top: 1px solid rgb(221,124,176);  border-bottom: 0px;  border-right: 0px; border-left: 2px solid rgb(118,31,77);}




