
/*Main Window*/

MyMainWindow{background-image: url(:/images/bg.png); background-repeat:no-repeat; background-color:#882459;}


/*Header Styles*/

QHBoxLayout#layoutHeader{ margin:0 10px 0 10px;}



/*Label Styles*/

QLabel#m_ptr_copyright_label{ background-image: url(:/images/copyright.png); margin:15px 0 5px 0; background-repeat:no-repeat; border:0; background-position:center;}

QLabel#m_ptr_title_label{color: rgb(255, 255, 255); font: 20pt Whitney Black; text-align:right; margin:2px 0 0 0;}

QLabel#m_ptr_clock_label{color: rgb(136, 36, 89); font: 18pt Whitney Semibold; text-align:center;}

QPushButton#m_ptr_home_button{background-image: url(:/images/homebtn.png); background-repeat:no-repeat; width:41px; height:36px; border:0;}

QPushButton::pressed#m_ptr_home_button{background-image: url(:/images/homebtn_pressed.png); background-repeat:no-repeat; width:41px; height:36px; border:0;}



/*Tabs Styles*/


QTabWidget#m_ptr_tab_widget{color: rgb(255, 255, 255); font: 14pt Whitney Black; border:0px; background-color:transparent; }

/*QTabWidget::Pane{padding:100px 0 0 25px;}*/

QTabWidget::Pane{border:0px}


QTabWidget::tab-bar {left: 60px;}

QTabBar::tab{ padding: 10px 0 10px 0; min-width:118px;  border-left: 1px solid qlineargradient(spread:pad, x1:0.5, y1:0, x2:0.5, y2:1, stop:0 rgba(121, 23, 74, 255), stop:1 rgba(0, 0, 0, 255)); border-right: 1px solid qlineargradient(spread:pad, x1:0.5, y1:0, x2:0.5, y2:1, stop:0 rgba(121, 23, 74, 255), stop:1 rgba(0, 0, 0, 255)); color: rgb(255, 255, 255);}


QTabBar::tab:selected{ background-color: qlineargradient(spread:pad, x1:0.506, y1:0.943591, x2:0.5, y2:0, stop:0 rgba(249, 193, 221, 255), stop:1 rgba(255, 255, 255, 255)); color: rgb(136, 36, 89);}

QTabBar::tab:hover, QTabBar::tab:selected:hover{ background-color: qlineargradient(spread:pad, x1:0.5, y1:0, x2:0.5, y2:1, stop:0 rgba(57, 0, 35, 255), stop:0.75 rgba(115, 21, 70, 255), stop:0.96 rgba(163, 58, 109, 255)); color: rgb(255, 255, 255);}



/*View Text Styles*/

QLabel#m_ptr_source_1_label, QLabel#m_ptr_source_2_label, QLabel#m_ptr_source_3_label, QLabel#m_ptr_source_4_label{color: rgb(255, 255, 255); font:bold 11pt Whitney;}


QLabel#m_ptr_source_1_labelBg, QLabel#m_ptr_source_2_labelBg, QLabel#m_ptr_source_3_labelBg, QLabel#m_ptr_source_4_labelBg{ background-image: url(:/images/seperator.png); background-repeat:no-repeat;  background-position:bottom; }


QLabel#view_1_source_1_not_defined_label, QLabel#view_1_source_2_not_defined_label, QLabel#view_1_source_3_not_defined_label, QLabel#view_1_source_4_not_defined_label{color: rgb(211, 89, 149); background-image: url(:/images/seperator.png); background-repeat:no-repeat;  background-position:bottom;  font:bold 11pt Whitney;}

