QLabel#lblsliderSource2{background-color: qlineargradient(spread:pad, x1:0.5, y1:0.991892, x2:0.5, y2:0, stop:0 rgba(134, 41, 0, 255), stop:0.8 rgba(241, 230, 51, 255), stop:0.87 rgba(253, 251, 218, 255), stop:0.97 rgba(253, 251, 218, 255), stop:1 rgba(241, 230, 51, 255));  border: 1px solid rgb(110,20,68);}

QLabel#lblsliderSource3{background-color: qlineargradient(spread:pad, x1:0.5, y1:0.991892, x2:0.5, y2:0, stop:0 rgba(54, 92, 6, 255), stop:0.8 rgba(168, 221, 100, 255), stop:0.87 rgba(224, 252, 189, 255), stop:0.97 rgba(253, 251, 218, 255), stop:1 rgba(205, 234, 168, 255));  border: 1px solid rgb(110,20,68);}

QLabel#lblsliderSource4{background-color: qlineargradient(spread:pad, x1:0.5, y1:0.991892, x2:0.5, y2:0, stop:0 rgba(0, 93, 116, 255), stop:0.8 rgba(51, 214, 255, 255), stop:0.87 rgba(211, 246, 255, 255), stop:0.97 rgba(232, 250, 255, 255), stop:1 rgba(230, 250, 255, 255));  border: 1px solid rgb(110,20,68);}


/*Other Resources Style*/

QLabel#lblsliderSourceUnselected{background-color: qlineargradient(spread:pad, x1:0.5, y1:0.991892, x2:0.5, y2:0, stop:0 rgba(121, 23, 74, 255), stop:0.75 rgba(254, 146, 220, 255), stop:0.890424 rgba(240, 185, 234, 255), stop:1 rgba(239, 130, 228, 255));  border: 1px solid rgb(110,20,68);}


/*Note Styles*/

EditNoteDialog{ background-color: rgba(29, 1, 17, 200);}

QPushButton#m_ptr_cancel_push_button{background-image: url(:/images/cancel_btn.png); background-repeat:no-repeat; width:80px; height:34px; border:0; color: rgb(255, 255, 255); font: bold 10pt Whitney;}

QPushButton::pressed#m_ptr_cancel_push_button{background-image: url(:/images/cancel_btn_pressed.png); }

QLabel#m_ptr_instructions_label{color: rgb(255, 255, 255); font: 15pt Whitney Black; }

