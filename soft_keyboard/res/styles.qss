QLabel#lblxAxis, QLabel#lblyAxis{color: rgb(255, 141, 197); font:bold 11pt Whitney;}


QLabel#lblBar{background-color: qlineargradient(spread:pad, x1:0, y1:0.5, x2:1, y2:0.502415, stop:0 rgba(121, 23, 74, 255), stop:1 rgba(49, 0, 30, 255)); border: 1px solid rgb(85,0,42);}

QLabel#lblSource1Selected{background-color:qlineargradient(spread:pad, x1:1, y1:0.5, x2:0, y2:0.478667, stop:0 rgba(134, 60, 0, 255), stop:1 rgba(241, 230, 51, 255)); border: 1px solid rgb(194,163,54); border-right: 1px solid rgb(108,31,66); border-bottom: 0px;}

QLabel#lblSource2Selected{background-color: qlineargradient(spread:pad, x1:1, y1:0.5, x2:0, y2:0.478667, stop:0 rgba(30, 3, 54, 255), stop:1 rgba(188, 149, 226, 255));}

QLabel#lblSource3Selected{background-color: qlineargradient(spread:pad, x1:1, y1:0.5, x2:0, y2:0.478667, stop:0 rgba(30, 3, 54, 255), stop:1 rgba(130, 170, 79, 255));}

QLabel#lblSource4Selected{background-color: qlineargradient(spread:pad, x1:1, y1:0.5, x2:0, y2:0.478667, stop:0 rgba(30, 3, 54, 255), stop:1 rgba(0, 204, 255, 255));}

QLabel#lblgraphSeperator{background-image: url(:/images/graphseperator.png); background-repeat:no-repeat; width:481px; height:1px; border:0;}



